{
	"name": "game-jolt-client",
	"version": "1.1.1",
	"scripts": {
		"start": "gulp watch",
		"dev": "gulp watch",
		"devd": "gulp watch --development",
		"translations": "gulp translations:compile",
		"client": "nw use 0.55.0-sdk && nw build/dev-client",
		"client-build": "gulp --production --client",
		"client-package": "gulp package-client --production --client",
		"client-release": "node scripts/client-release.js",
		"ssr-server": "gulp --ssr server",
		"ssr-client": "gulp --ssr client",
		"ssr": "cd src && node server.js",
		"check": "tsc --noEmit",
		"analyze-bundle": "gulp --analyze-bundle",
		"lint": "eslint --ext .js,.vue src",
		"build:app": "gulp --production --app",
		"build:web": "vite build --outDir build/web",
		"build:server": "vite build --outDir build/server --ssr src/auth/server.ts"
	},
