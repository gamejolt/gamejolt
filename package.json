{
	"name": "game-jolt-client",
	"version": "1.14.6",
	"scripts": {
		"check": "tsc --noEmit",
		"lint": "eslint --ext .js,.vue src",
		"tsscript": "ts-node -P scripts/script.tsconfig.json",
		"client-release": "yarn tsscript scripts/client-release.ts",
		"vite": "yarn tsscript scripts/build/vite-wrapper.ts",
		"dev": "yarn vite --build-type serve-hmr",
		"devd": "yarn dev --environment development",
		"build": "yarn vite build --build-type build",
		"devd-z": "yarn devd --section z",
		"build:dev": "yarn tsscript scripts/build-and-serve.ts",
		"build:devd": "yarn tsscript scripts/build-and-serve.ts --environment development --http --host 0.0.0.0 --port 8080",
		"app:build": "yarn build --platform mobile --section editor",
		"client:dev": "yarn dev --platform desktop",
		"client:devd": "yarn devd --platform desktop",
		"client:build": "yarn tsscript scripts/build/desktop-app/index.ts --build",
		"client:package": "yarn tsscript scripts/build/desktop-app/index.ts --package",
		"client": "nw use 0.62.2-sdk && nw .",
		"ssr:client": "yarn build --platform web",
		"ssr:server": "yarn build --platform ssr",
		"ssr": "node ssr/server.js",
		"pre-commit": "yarn tsscript scripts/git-pre-commit.ts"
	},
