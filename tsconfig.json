{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"moduleResolution": "node",
		"isolatedModules": true,
		"strict": true,
		"experimentalDecorators": true,
		"strictFunctionTypes": false,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"allowJs": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"importHelpers": true,
		"lib": ["esnext", "dom", "dom.iterable", "scripthost"]
	},
	"exclude": ["build", "node_modules", ".cache"]
}
